{
  "name": "streaming-video-sandbox",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "ssh://git@github.com/muyuu/streaming-video-sandbox.git",
  "author": "muyuu <anticyborg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "storybook": "run-p storybook:start watch:css",
    "storybook:start": "start-storybook -s ./dist -p 9001",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "run-s build:css build:js",
    "build:js": "webpack --mode production",
    "build:css": "postcss src/registry.css --env production --dir dist/",
    "prewatch": "yarn clean",
    "watch": "run-p watch:css watch:js",
    "watch:js": "webpack-dev-server --mode development",
    "watch:css": "postcss src/registry.css --watch --verbose --env development --dir dist/"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "hls.js": "^0.14.16",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "shaka-player": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@types/classnames": "^2.2.9",
    "@types/hls.js": "^0.13.3",
    "@types/node": "^13.1.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.2",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "cssnano": "^4.1.10",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "postcss-hash": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "stylelint": "^13.0.0",
    "stylelint-no-unused-selectors": "^1.0.24",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
